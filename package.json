{
  "name": "cf-component-modal",
  "version": "0.0.0",
  "description": "CloudFlare Modal Component",
  "main": "lib/index.js",
  "directories": {
    "example": "example",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "babel src -d lib",
    "example": "browserify example/script.js -o example/bundle.js -t babelify --debug",
    "format": "jsfmt -w *.js src/ test/",
    "lint": "eslint *.js src/ test/",
    "prepublish": "npm run build",
    "test": "karma start"
  },
  "keywords": [
    "cloudflare",
    "component",
    "modal"
  ],
  "author": "James Kyle <jkyle@cloudflare.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.1.1",
    "babel-core": "^6.0.20",
    "babel-preset-cf": "^1.0.1",
    "babelify": "^7.2.0",
    "browserify-istanbul": "^0.2.1",
    "chai": "^3.4.1",
    "defined": "^1.0.0",
    "eslint": "^1.8.0",
    "jsfmt": "^0.5.2",
    "karma": "^0.13.15",
    "karma-beep-reporter": "^0.1.4",
    "karma-browserify": "^4.4.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-safari-launcher": "^0.1.1",
    "karma-sauce-launcher": "^0.3.0",
    "karma-tape-reporter": "^1.0.3",
    "minimist": "^1.2.0",
    "mocha": "^2.3.3",
    "react-addons-test-utils": "^0.14.2"
  },
  "dependencies": {
    "react": "^0.14.2",
    "react-addons-css-transition-group": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-gateway": "^1.1.0",
    "react-modal2": "^2.0.0"
  }
